{
  "name": "app-ssr",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "tsx server/serve.ts",
    "build": "run-s build:update-bulma-colors build:client build:server",
    "build:update-bulma-colors": "bulma-css-vars",
    "build:client": "cross-env NODE_ENV=production vite build --ssrManifest --outDir dist/client",
    "build:server": "cross-env NODE_ENV=production vite build --ssr src/entry-server.ts --outDir dist/server",
    "generate": "run-s build:update-bulma-colors generate:build",
    "generate:build": "NODE_ENV=production cross-env tsx server/generate.ts",
    "start": "cross-env NODE_ENV=production tsx server/serve.ts",
    "test": "run-p test:*",
    "test:tsc": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@cssninja/bulma-css-vars": "0.9.2",
    "@fontsource-variable/fira-code": "5.0.18",
    "@fontsource-variable/montserrat": "5.0.19",
    "@fontsource-variable/roboto-flex": "5.0.15",
    "@nuxt/devalue": "2.0.2",
    "@unhead/addons": "1.9.9",
    "@unhead/ssr": "1.9.9",
    "@unhead/vue": "1.9.9",
    "@vueuse/core": "10.9.0",
    "@vueuse/router": "10.9.0",
    "bulma": "npm:@cssninja/bulma@0.9.4",
    "compression": "1.7.4",
    "cross-env": "7.0.3",
    "defu": "6.1.4",
    "focus-trap": "7.5.4",
    "focus-trap-vue": "4.0.3",
    "h3": "1.11.1",
    "html-minifier-terser": "7.2.0",
    "iconify-icon": "2.1.0",
    "listhen": "1.7.2",
    "npm-run-all": "4.1.5",
    "picocolors": "1.0.0",
    "pinia": "2.1.7",
    "serve-static": "1.15.0",
    "std-env": "3.7.0",
    "tslib": "2.6.2",
    "tsx": "4.9.1",
    "ufo": "1.5.3",
    "unhead": "1.9.9",
